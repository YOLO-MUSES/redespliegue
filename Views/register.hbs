<h1>Usuarios nuevos</h1>

<div class="row">
    <div class="col-md-6">
        <form action="/auth/register" method="post">
        <input type="hidden" name="_csrf" value="{{csrfToken}}">
            <input type="email" name="email" placeholder="ingrese un correo" >
            <input type="text"  name="userName" placeholder="ingrese su usuario" >
            <input type="tel" name="Telefono" >
            <input type="password" name="password" placeholder="ingrese su contraseña">
             <input type="password" name="repassword" placeholder="re ingrese su contraseña" >
            <button class="btn btn-success" type="submit" >registrarse</button>
        </form>
    </div>
</div>
